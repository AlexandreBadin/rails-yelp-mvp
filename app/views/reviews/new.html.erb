<div class="container">
  <h1>Let me a review</h1>

  <%= simple_form_for [ @restaurant, @review ] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "Submit review", class: "btn btn-success" %>
    <%= link_to 'Back to restaurant list', root_path(@restaurant), class: 'btn btn-primary'%>
  <% end %>
</div>
